  "author": "Kim Biesbjerg <kim@biesbjerg.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/biesbjerg/ngx-translate-extract/issues"
  },
  "homepage": "https://github.com/biesbjerg/ngx-translate-extract",
  "engines": {
    "node": ">=4.3.2"
  },
  "config": {},
  "devDependencies": {
    "@types/chai": "3.4.35",
    "@types/glob": "5.0.30",
    "@types/mocha": "2.2.40",
    "@types/cheerio": "0.22.1",
    "@types/chalk": "0.4.31",
